.fullbg {
  position: fixed;
  top: 132rpx;
  z-index: 1;
  width: 100%;
  /* height: 100%; */
  background: rgb(1, 1, 1);
  transition: all 2s;
  opacity: 0;
}

.fullopacity {
  opacity: 0.5;
}

.nav-son.active .content_nav {
  color: rgb(255, 152, 0);
}

.nav-son.active .icon-nav{
  border: 10rpx solid transparent;
  border-top: 10rpx solid rgb(255, 152, 0);
}

@keyframes slidown {
  from {
    transform: translateY(-100%);
  }

  to {
    transform: translateY(0%);
  }
}

.slidown {
  width: 100%;
  display: block;
  animation: slidown 0.7s ease-in both;
  position: absolute;
}

@keyframes slidup {
  from {
    transform: translateY(0%);
  }

  to {
    transform: translateY(-100%);
  }
}

.slidup {
  display: block;
  animation: slidup 0.7s ease-in both;
}

.disappear {
  display: none;
}

