}

.slidedown {
  transform: translateY(0%);
}

.temp view {
  /* border-bottom: 1rpx solid #d1d3d4; */
  font-size: 28rpx;
  color: #666;
}

.pxTable {
  border-bottom: 1rpx solid #f4f4f4;
}

.quyu {
  position: relative;
  /* 区域的筛选：默认状态下不显示 */
  display: none;
  /* height: 450rpx; */
  height: 485rpx;
  z-index: 5;
}
.quyu2{
  text-align: center;
  width: 100%;
  max-height: 485rpx;
}

.quyu .qy {
  overflow-y: scroll;
  float: left;
  height: 82%;
  line-height: 80rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #717273;
  border-right: 1rpx solid #f4f4f4;
}

.qy view {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  text-align: center;
  /* padding-left: 45rpx; */
  border-bottom: 1rpx solid #f4f4f4;
}

.quyu-left {
  border-bottom: 1rpx solid #f4f4f4;
  width: 50%;
  background: #fff;
}

.quyu-center {
  border-bottom: 1rpx solid #f4f4f4;
  width: 50%;
  background: #fff;
}

.quyu-right {
  width: 100%;
  background: #fff;
}

.quyu-right view {
  border-bottom: 1rpx solid #efefef;
  display: flex;
  justify-content: center;
  align-items: center;
}

.quyu-right image {
  width: 34rpx;
  height: 34rpx;
  margin-left: 30rpx;
}

