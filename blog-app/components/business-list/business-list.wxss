.progress{
  display: flex;
  justify-content: flex-start;
  width: 300px;
  align-items: center;
  position: relative;
}
.content_text .progress .progress_left{
  background-size:100% 100%;
  -moz-background-size:100% 100%;
  background: url('data:image/png;base64,') no-repeat;
  height: 25px;
  width: 60px; 
}
.progress_left progress{
  margin-top: 5px;
  margin-left: 5px;
  width: 36px;
}
.progress_right{
  /* width: 50rpx */
}
.pro_per{
  position: absolute;
  font-size: 18rpx;
  top: 12rpx;
  left: 14rpx;
  color: #222;
}
.content{
	width: 100%;
	height: 260rpx;
	background:rgb(255, 255, 255);
	border-bottom: 2rpx solid rgb(240, 238, 238);
	display: flex;
  justify-content: space-between;
 	align-content: center;
  /* padding-bottom:20rpx;  */
}
.content navigator{
  width: 85%;
  height: 100%;
  display: flex;
  justify-content: flex-start;
}
.content .content_img{
	padding-left: 44rpx;
	padding-top: 30rpx;
  padding-right: 20rpx;
	width: 264rpx;
	height: 220rpx;
}
.content .content_img image{
	width: 100%;
	height: 100%;
}

/* 中间内容 */
 .content .content_text{
  width: 350rpx;
  position: relative;
	font-size: 24rpx;
	color: rgb(136, 136, 136);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 250rpx;
}
.content .content_text .text{
	padding-right: 4rpx;
}

.content .content_text_title {
  margin-top: 30rpx;
  /* padding-bottom: 10rpx; */
  color: #101010;
  font-size: 30rpx;
  width: 100%;
  word-break: break-all;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* font-size: 24rpx; */
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.content .content_text_cente{
	display: flex;
  justify-content: flex-start;
 	align-content: center;
} 

 .content .content_text_grade image{
	width: 28rpx;
	height: 28rpx;
}
 icon 
.content .content_icon{
	width: 18%;
	height: 50rpx;
	padding-top: 65rpx;
}
.content .content_icon button::after{
  border: none;
}
.content .content_icon button{
  background: #fff;
}
.content .content_icon image{
	width: 90rpx;
	height: 90rpx;
} 